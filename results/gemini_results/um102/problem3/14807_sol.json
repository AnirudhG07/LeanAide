{
  "type": "theorem",
  "name": "Gradient of f(x, y) = x^3 - 3x + y^2",
  "hypothesis": [
    {
      "type": "let",
      "variable": "f(x, y)",
      "value": "x^{3}-3 x+y^{2}",
      "kind": "function from R^2 to R"
    }
  ],
  "conclusion": "The gradient of f is zero at the points (1, 0) and (-1, 0).",
  "status": "proved",
  "proof": {
    "type": "proof",
    "steps": [
      {
        "type": "let",
        "variable": "f(x, y)",
        "value": "x^3 - 3x + y^2",
        "kind": "function from R^2 to R"
      },
      {
        "type": "assert",
        "claim": "The gradient of f(x, y) is given by (3x^2 - 3)i + (2y)j.",
        "proof-method": "Direct calculation",
        "calculation": [
          "∇f(x, y) = (∂f/∂x)i + (∂f/∂y)j",
          "∇f(x, y) = (3x^2 - 3)i + (2y)j"
        ]
      },
      {
        "type": "assert",
        "claim": "The gradient of f is zero at (1, 0) and (-1, 0).",
        "proof-method": "Solving the equation ∇f(x, y) = 0",
        "calculation": [
          "∇f(x, y) = 0",
          "(3x^2 - 3)i + (2y)j = 0",
          "3x^2 - 3 = 0 and 2y = 0",
          "x = 1 or x = -1, and y = 0",
          "(x, y) = {(1, 0), (-1, 0)}"
        ]
      },
      {
        "type": "conclude",
        "statement": "Therefore, the gradient of f is zero at the points (1, 0) and (-1, 0)."
      }
    ]
  }
}